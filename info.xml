<?xml version="1.0"?>
<extension key="org.civicoop.relataionshipuploadfix" type="module">
  <file>relataionshipuploadfix</file>
  <name>Relationship Upload Fix</name>
  <description>This extension contains a fix for relationship creation and custom file uploads. The civicrm core (4.4.x) doesn't handle file uploads when a relationship is created. This extension will fix that problem.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Jaap Jansma - CiviCooP</author>
    <email>helpdesk@civicoop.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2015-01-26</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.2</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <civix>
    <namespace>CRM/Relataionshipuploadfix</namespace>
  </civix>
</extension>
